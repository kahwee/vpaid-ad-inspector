    "watchify": "webpack --progress --watch",
    "test": "cross-env NODE_ENV=test karma start",
    "coveralls": "cat ./coverage/report-lcov/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vpaid-ad": "^4.0.0"
  },
  "standard": {
    "ignore": [
      "/dist",
      "/coverage",
      "/reports"
    ],
    "env": [
      "mocha"
    ],
    "globals": [
      "MouseEvent",
      "expect"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "html-loader": "^0.5.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "standard": "^10.0.3",
    "webpack": "^4.35.0"
